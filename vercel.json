{
    "version": 2,
    "builds": [
      {
        "src": "mysite/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.13" }
      }
    
    ],
    "routes": [     
      {
        "src": "/(.*)",
        "dest": "mysite/wsgi.py"
      }
    ]
  }